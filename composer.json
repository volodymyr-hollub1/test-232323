{
    "name": "vova-holub/dns-resolver-library",
    "description": "Library for working with DNS.",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
          "VH\\DnsResolver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "VH\\DnsResolver\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Vova Holub"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.13"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2"
    }
}
